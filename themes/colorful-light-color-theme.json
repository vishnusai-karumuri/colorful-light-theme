{
    "name": "Better Light Theme",
    "type": "light",
    "colors": {
        "editor.background": "#ffffff",
        "editor.foreground": "#2d2b55",
        "activityBar.background": "#f9f9f9",
        "activityBar.foreground": "#2d2b55",
        "sideBar.background": "#f3f3f3",
        "sideBar.foreground": "#2d2b55",
        "titleBar.activeBackground": "#218c4d",
        "titleBar.activeForeground": "#ffffff",
        "statusBar.background": "#218c4d",
        "statusBar.foreground": "#ffffff",
        "tab.activeBackground": "#ffffff",
        "tab.activeForeground": "#2d2b55",
        "tab.inactiveBackground": "#f3f3f3"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#a0a1a7"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "string.interpolated"
            ],
            "settings": {
                "foreground": "#16a085"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#8e44ad"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#2980b9"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other"
            ],
            "settings": {
                "foreground": "#c0392b"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#e67e22"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#218c4d"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#34495e"
            }
        }
    ]
}